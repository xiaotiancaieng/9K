a=gg.prompt({"输入你的卡密"}),{'3kssb'},{'text'})
if a==nil then os.exit() end
if a[1] == '3kssb' then gg.alert("尊敬的用户您的卡密正确") end
if a[1] ～= '3kssc' then gg.alert("卡密错误请找3876527033获取卡密") os.exit() end

local UIS = game:GetService("UserInputService")

local function MakeDraggable(topbar, obj)
    local dragging, dragInput, dragStart, startPos
    local function Update(input)
        local delta = input.Position - dragStart
        obj.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
    end
    topbar.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            dragging = true
            dragStart = input.Position
            startPos = obj.Position
            input.Changed:Connect(function() if input.UserInputState == Enum.UserInputState.End then dragging = false end end)
        end
    end)
    topbar.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
            dragInput = input
        end
    end)
    UIS.InputChanged:Connect(function(input) if input == dragInput and dragging then Update(input) end end)
en

function YtPr()
    local sg = Instance.new("ScreenGui", game.CoreGui)
    sg.Name = "YoutubeGui"
    sg.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

    local frame = Instance.new("Frame", sg)
    frame.BackgroundColor3 = Color3.new(1,1,1)
    frame.Position = UDim2.new(0.094,0,0.498,0)
    frame.Size = UDim2.new(0.26,0,0.172,0)
    Instance.new("UICorner", frame)
    MakeDraggable(frame, frame)

    local img = Instance.new("ImageLabel", frame)
    img.BackgroundColor3 = Color3.new(1,1,1)
    img.Position = UDim2.new(0.058,0,0.07,0)
    img.Size = UDim2.new(0.295,0,0.85,0)
    img.Image = "http://www.roblox.com/asset/?id=12130059538"
    local imgCorner = Instance.new("UICorner", img)
    imgCorner.CornerRadius = UDim.new(0,60)
    Instance.new("UIStroke", img).Color = Color3.new(1,0,0) -- 红色边框

    local txt = Instance.new("TextLabel", frame)
    txt.BackgroundColor3 = Color3.new(1,1,1)
    txt.BackgroundTransparency = 1
    txt.Position = UDim2.new(0.401,0,0.17,0)
    txt.Size = UDim2.new(0.535,0,0.31,0)
    txt.Font = Enum.Font.Highway
    txt.Text = "自制脚本"
    txt.TextColor3 = Color3.new(0,0,0)
    txt.TextScaled = true
    txt.TextXAlignment = Enum.TextXAlignment.Left

    local btn = Instance.new("TextButton", frame)
    btn.BackgroundColor3 = Color3.new(1,0,0)
    btn.Position = UDim2.new(0.401,0,0.48,0)
    btn.Size = UDim2.new(0.535,0,0.35,0)
    btn.Font = Enum.Font.SourceSans
    btn.Text = "1"
    btn.TextColor3 = Color3.new(1,1,1)
    btn.TextScaled = true
    Instance.new("UICorner", btn)

    
    game.Debris:AddItem(sg, 10)
    btn.MouseButton1Click:Connect(function()
        game.Debris:AddItem(sg, 0)
        setclipboard("https://b23.tv/tavz4ym")
    end)
end

function MakePrGui()
    local sg = Instance.new("ScreenGui", game.Players.LocalPlayer:WaitForChild("PlayerGui"))
    sg.Name = "PrEz"
    sg.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

    
    local back = Instance.new("ImageLabel", sg)
    back.Name = "Back"
    back.BackgroundColor3 = Color3.new(38/255,38/255,38/255)
    back.Size = UDim2.new(0.628,0,1,0)
    
    back.Image = "rbxassetid://3876527033"
    local robloxLogo = Instance.new("ImageLabel", sg)
    robloxLogo.Name = "RobloxLogo"
    robloxLogo.BackgroundColor3 = Color3.new(38/255,38/255,38/255)
    robloxLogo.BackgroundTransparency = 1

    local vips = {Instance.new("ImageLabel", sg) for _=1,7}
    for i,vip in ipairs(vips) do
        vip.Name = i==1 and "vip" or "vip"..i
        MakeDraggable(vip, vip)
    end

    local closeBtn = Instance.new("TextButton", sg)
    closeBtn.Name = "CloseButton"
    closeBtn.Text = "关闭"
    closeBtn.BackgroundColor3 = Color3.new(1,0,0)
    closeBtn.MouseButton1Click:Connect(function() sg:Destroy() end)
end

LBL.TextSize = 14LBL.TextWrapped = true
LBL.Visible = true
local FpsLabel = LBL
local Heartbeat = 
game:GetService("RunService").Heartbeat
local LastIteration, Start
local FrameUpdateTable = { }
local function HeartbeatUpdate()	
LastIteration = tick()	
for Index = #FrameUpdateTable, 1, -1 do		
FrameUpdateTable[Index + 1] = 
(FrameUpdateTable[Index] >= LastIteration - 1) and 
FrameUpdateTable[Index] or nil	
end	
FrameUpdateTable[1] = LastIteration	
local CurrentFPS = (tick() - Start >= 1 and 
#FrameUpdateTable) or (#FrameUpdateTable / 
(tick() - Start))	
CurrentFPS = CurrentFPS - CurrentFPS % 1	
FpsLabel.Text = ("北京时
间:"..os.date("%H").."时"..os.date("%M").."分"..os.date(
"%S"))

end

Start = tick()
Heartbeat:Connect(HeartbeatUpdate)
